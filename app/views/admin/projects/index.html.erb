<div class="p-30">
  <%= link_to new_admin_project_path do %>
    <div class="btn btn-primary">New Project</div>
  <% end %>
  <% @projects.each do |project| %>
    <div>
      <p>
        <%= project.name %>
        <%= link_to 'Edit',  edit_admin_project_path(project)%>
        <%= link_to 'Delete',  admin_project_path(project), method: :delete %>
      </p>
    </div>
  <% end %>
</div>




