<%= simple_form_for [:admin, project] do |f| %>
  <%= f.input :name %>
  <%= f.input :location %>
  <%= f.input :client %>
  <%= f.input :architect %>
  <%= f.input :services %>
  <%= f.input :content, input_html: { rows: 10 } %>
  <%= f.input :banner %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.submit %>
<% end %>
  